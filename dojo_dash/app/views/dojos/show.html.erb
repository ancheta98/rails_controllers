<h1> <%=@dojo.branch%> </h1>
<h3>Address: <%=@dojo.street%> </h3>
<h3>City: <%=@dojo.city%></h3>
<h3>State: <%=@dojo.state%> </h3>
<table>
    <tr>
        <th>First Name</th>
        <th>Last name</th>
        <th>Email</th>
        <th>Actions</th>
    </tr>
    <%@dojo.students.each do |s|%>
    <tr>
        <td><%=s.first_name%></td>
        <td><%=s.last_name%></td>
        <td><%=s.email%></td>
        <td><a href="/dojos/<%=@dojo.id%>/students/<%=s.id%>">Show</a><a href="/dojos/<%=@dojo.id%>/students/<%=s.id%>/edit">|Edit</a><a href="/dojos/<%=@dojo.id%>/students/<%=s.id%>" data-method="delete">|Destroy</a></td>
    </tr>
    <%end%>
</table>
<a href="/dojos">Home</a>|<a href="dojos/<%=@dojo.id%>">Edit</a>|<a href="/dojos/<%=@dojo.id%>/students/new">Add student</a>
